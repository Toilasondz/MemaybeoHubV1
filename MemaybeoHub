-- ====================================================================
-- 1. T·∫¢I V√Ä KH·ªûI T·∫†O TH∆Ø VI·ªÜN RAYFIELD
-- ====================================================================
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- ====================================================================
-- 2. KHAI B√ÅO BI·∫æN TO√ÄN C·ª§C CHO T√çNH NƒÇNG
-- ====================================================================
-- Bi·∫øn ki·ªÉm so√°t tr·∫°ng th√°i Infinite Jump
_G.InfinJump = false
-- Bi·∫øn ki·ªÉm so√°t vi·ªác l·∫Øng nghe s·ª± ki·ªán ph√≠m (ƒë·ªÉ tr√°nh l√£ng ph√≠ t√†i nguy√™n)
_G.InfinJumpStarted = nil

-- ====================================================================
-- 3. T·∫†O C·ª¨A S·ªî V√Ä C·∫§U H√åNH (ƒê√£ t·∫Øt Key System)
-- ====================================================================
local Window = Rayfield:CreateWindow{
    Name = "Youtube Hub", -- T√™n ƒë√£ ƒë∆∞·ª£c thay ƒë·ªïi/t·ªëi ∆∞u
    LoadingTitle = "Example Hub",
    LoadingSubtitle = "by 1_F0",
    ConfigurationSaving = {
        Enabled = false,
        FolderName = nil,
        FileName = "Youtubeexamplehub"
    },
    Discord = {
        Enabled = false,
        Invite = "vegax", 
        RememberJoins = true 
    },
    KeySystem = false -- ƒê√£ t·∫Øt Key System theo y√™u c·∫ßu tr∆∞·ªõc ƒë√≥
}

-- T·∫°o Tab v√† Section
local MainTab = Window:CreateTab("üè† Home", nil) -- Title, Image
local MainSection = MainTab:CreateSection("Main")


local Button = MainTab:CreateButton({
    Name = "Infinite Jump: OFF", -- T√™n ban ƒë·∫ßu
    Callback = function(self) -- 'self' l√† ƒë·ªëi t∆∞·ª£ng n√∫t b·∫•m ƒë∆∞·ª£c nh·∫•n

        -- ƒê·∫£o ng∆∞·ª£c tr·∫°ng th√°i Infinite Jump
        _G.InfinJump = not _G.InfinJump

        -- C·∫≠p nh·∫≠t t√™n n√∫t tr√™n UI
        local status = _G.InfinJump and "ON" or "OFF"
        self:SetName("Infinite Jump: " .. status)
        
        -- N·∫øu ƒë√¢y l√† l·∫ßn ƒë·∫ßu ti√™n t√≠nh nƒÉng ƒë∆∞·ª£c b·∫≠t, kh·ªüi t·∫°o logic l·∫Øng nghe ph√≠m
        if _G.InfinJumpStarted == nil then
            _G.InfinJumpStarted = true -- ƒê·∫£m b·∫£o kh·ªëi n√†y ch·ªâ ch·∫°y m·ªôt l·∫ßn

            local plr = game:GetService('Players').LocalPlayer
            local m = plr:GetMouse()

            m.KeyDown:Connect(function(k)
                -- Ki·ªÉm tra: T√≠nh nƒÉng c√≥ b·∫≠t V√Ä Ph√≠m Spacebar c√≥ ƒë∆∞·ª£c nh·∫•n kh√¥ng (k.KeyCode.Space)
                if _G.InfinJump and k.KeyCode == Enum.KeyCode.Space then
                    local humanoid = plr.Character and plr.Character:FindFirstChildOfClass('Humanoid')
                    if humanoid then
                        -- Bu·ªôc Humanoid chuy·ªÉn sang tr·∫°ng th√°i nh·∫£y
                        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                        task.wait() -- Ch·ªù m·ªôt ch√∫t
                        -- Bu·ªôc Humanoid chuy·ªÉn sang tr·∫°ng th√°i Seated ƒë·ªÉ reset tr·∫°ng th√°i nh·∫£y
                        humanoid:ChangeState(Enum.HumanoidStateType.Seated)
                    end
                end
            end)
        end
        
        -- Th√¥ng b√°o tr·∫°ng th√°i (d√πng Rayfield:Notify)
        Rayfield:Notify({
            Title = "Infinite Jump",
            Content = _G.InfinJump and "ƒê√£ k√≠ch ho·∫°t Infinite Jump!" or "ƒê√£ t·∫Øt Infinite Jump.",
            Duration = 3,
        })
    end,
})

-- ====================================================================
-- 5. TH√îNG B√ÅO KH·ªûI CH·∫†Y BAN ƒê·∫¶U (T√πy ch·ªçn)
-- ====================================================================

Rayfield:Notify({
    Title = "You executed the script!",
    Content = "very good gui",
    Duration = 5,
    Image = nil,
    Actions = { 
        Ignore = {
            Name = "Okay!",
            Callback = function()
                print("Ng∆∞·ªùi d√πng ƒë√£ nh·∫•p Okay!")
            end
        }
    }
})

local Button = MainSection:CreateButton({ -- ƒê·∫∑t n√∫t trong MainSection
    Name = "Infinite Jump: OFF", -- T√™n ban ƒë·∫ßu
    Callback = function(self) -- 'self' l√† ƒë·ªëi t∆∞·ª£ng n√∫t b·∫•m

        -- ƒê·∫£o ng∆∞·ª£c tr·∫°ng th√°i Infinite Jump
        _G.InfinJump = not _G.InfinJump

        -- C·∫≠p nh·∫≠t t√™n n√∫t tr√™n UI
        local status = _G.InfinJump and "ON" or "OFF"
        self:SetName("Infinite Jump: " .. status)
        
        -- N·∫øu ƒë√¢y l√† l·∫ßn ƒë·∫ßu ti√™n t√≠nh nƒÉng ƒë∆∞·ª£c b·∫≠t, kh·ªüi t·∫°o logic l·∫Øng nghe ph√≠m
        if _G.InfinJumpStarted == nil then
            _G.InfinJumpStarted = true -- Kh·ªëi n√†y ch·ªâ ch·∫°y m·ªôt l·∫ßn

            local plr = game:GetService('Players').LocalPlayer
            local m = plr:GetMouse()

            m.KeyDown:Connect(function(k)
                -- Ki·ªÉm tra: T√≠nh nƒÉng c√≥ b·∫≠t V√Ä Ph√≠m Spacebar c√≥ ƒë∆∞·ª£c nh·∫•n kh√¥ng (m√£ byte 32)
                if _G.InfinJump and k:byte() == 32 then
                    local humanoid = plr.Character and plr.Character:FindFirstChildOfClass('Humanoid')
                    if humanoid then
                        humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
                        task.wait() -- D√πng task.wait() hi·ªán ƒë·∫°i
                        humanoid:ChangeState(Enum.HumanoidStateType.Seated) -- Reset tr·∫°ng th√°i nh·∫£y
                    end
                end
            end)
        end
        
        -- Th√¥ng b√°o tr·∫°ng th√°i
        Rayfield:Notify({
            Title = "Youtube Hub",
            Content = _G.InfinJump and "Infinite Jump Activated!" or "Infinite Jump Disabled.",
            Duration = 5,
        })
    end,
})

